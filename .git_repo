#!/bin/bash

GIT_PROJECT=""
Git_Top="$HOME/Work/git/"

case "$1" in
    foo)
        GIT_PROJECT="dbianco/foo"
        ;;
    api)
        GIT_PROJECT="vagrant/local-group/www/api"
        ;;
    gis)
        GIT_PROJECT="dbianco/geospatial-data"
        ;;
    *)
        echo >&2 "Invalid repo"
        ;;
esac

cd ${Git_Top}${GIT_PROJECT}

